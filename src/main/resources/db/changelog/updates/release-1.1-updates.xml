<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="release-1.1-updates" author="nikita_glushko">
        <preConditions onFail="MARK_RAN">
            <dbms type="postgresql"/>
            <sqlCheck expectedResult="document-flow">SELECT current_database()</sqlCheck>

            <tableExists tableName="document_types"/>


        </preConditions>
        <update tableName="document_types">
            <column name="org_id" value="1"/>
        </update>
    </changeSet>
</databaseChangeLog>
