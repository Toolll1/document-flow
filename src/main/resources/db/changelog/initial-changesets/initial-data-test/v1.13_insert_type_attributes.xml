<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="v1.13_insert_type_attributes" author="svyat">
        <preConditions onFail="MARK_RAN">
            <dbms type="postgresql"/>
            <sqlCheck expectedResult="${database.name}">SELECT current_database()</sqlCheck>
            <tableExists tableName="type_attributes"/>
        </preConditions>
        <insert tableName="type_attributes">
            <column name="type_id" value="1"/>
            <column name="attribute_id" value="1"/>
        </insert>

    </changeSet>
</databaseChangeLog>