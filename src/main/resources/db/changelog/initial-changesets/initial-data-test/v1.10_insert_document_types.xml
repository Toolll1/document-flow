<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="v.10_insert_document_types" author="svyat">
        <preConditions onFail="MARK_RAN">
            <dbms type="postgresql"/>
            <sqlCheck expectedResult="${database.name}">SELECT current_database()</sqlCheck>
            <tableExists tableName="document_types"/>
        </preConditions>
        <insert tableName="document_types">
            <column name="name" value="Type_Doc1"/>
            <column name="agreement_type" value="QUORUM"/>
            <column name="org_id" value="1"/>
        </insert>


    </changeSet>
</databaseChangeLog>