<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="v1.7_insert_document_process_comment" author="sveta_klimova">
        <preConditions onFail="MARK_RAN">
            <dbms type="postgresql"/>
            <sqlCheck expectedResult="${database.name}">SELECT current_database()</sqlCheck>
            <tableExists tableName="document_process_comment"/>
        </preConditions>
        <insert tableName="document_process_comment">
            <column name="document_process_id" value="1"/>
            <column name="user_id" value="2"/>
            <column name="comment_content" value="Text comment"/>
            <column name="created_at" value="NOW()"/>
        </insert>
        <insert tableName="document_process_comment">
            <column name="document_process_id" value="2"/>
            <column name="user_id" value="2"/>
            <column name="comment_content" value="Text comment 2"/>
            <column name="created_at" value="NOW()"/>
        </insert>
    </changeSet>
</databaseChangeLog>